{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "jest/expect-expect": [
      "warn",
      {
        "assertFunctionNames": ["expect", "runExtractionTest"],
        "additionalTestBlockFunctions": []
      }
    ]
  },
  "plugins": ["@typescript-eslint"],
  "extends": "seek",
  "ignorePatterns": ["out", "dist", "**/*.d.ts"]
}
